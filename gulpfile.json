{
  "common": {
    "artifacts": "artifacts",
    "components": ":{{common.dest}}/components",
    "names": {
      "wiredep": "wiredep"
    }
  },
  "options": {
    "package": {
      "src": [
        "dist/**/*.js"
      ]
    },
    "publish": {
      "npm": {
        "registry": "https://npm.nativecode.com"
      }
    },
    "wiredep": {
      "src": [
        "html"
      ],
      "options": {}
    }
  },
  "plugins": {
    "babel": {
      "presets": "es2015"
    },
    "badge": {
      "link": "https://build.nativecode.com/viewType.html?buildTypeId=nativecode_opensource_buildconfig_continuous",
      "src": "https://img.shields.io/teamcity/http/teamcity.jetbrains.com/s/bt345.svg?maxAge=2592000&style=flat-square&label=continuous"
    }
  }
}