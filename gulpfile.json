{
  "common": {
    "artifacts": "artifacts"
  },
  "options": {
    "package": {
      "context": "package.json",
      "dest": ":{{common.artifacts}}"
    },
    "publish": {
      "src": ["package.json"],
      "npm": {
        "registry": "https://npm.nativecode.com"
      }
    }
  },
  "plugins": {
    "babel": {
      "presets": "es2015"
    }
  }
}